{
  "require": {
    "craftcms/cms": "3.0.35",
    "vlucas/phpdotenv": "^2.4.0",
    "nystudio107/craft-twigpack": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "require-dev": {
    "laravel/homestead": "^7.20"
  }
}
